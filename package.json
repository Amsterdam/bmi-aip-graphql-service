{
	"name": "bmi-aip-graphql-service",
	"version": "0.0.1",
	"description": "A GraphQL service developed with NestJS in service of the frontend of the \"Amsterdam Inspectie Portaal\" app",
	"author": "Gemeente Amsterdam",
	"license": "MPL-2.0",
	"scripts": {
		"prestart": "npx @olegjs/on-change --file schema.prisma node_modules/.bin/prisma generate",
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:test": "nest start --watch",
		"start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"ci:test": "jest --ci --reporters=default",
		"ci:lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
		"prepare": "husky install",
		"console": "node dist/src/console.js",
		"prisma": "prisma",
		"generate:gisib-api-types": "node_modules/.bin/ts-node scripts/generate-gisib-api-types/index.ts",
		"type-check": "tsc -p tsconfig.json"
	},
	"dependencies": {
		"@apollo/server": "^4.8.1",
		"@casl/ability": "^5.4.3",
		"@casl/prisma": "^1.4.1",
		"@golevelup/nestjs-graphql-request": "^0.1.13",
		"@nestjs/apollo": "^12.0.7",
		"@nestjs/axios": "^3.0.0",
		"@nestjs/cache-manager": "^2.1.0",
		"@nestjs/common": "^10.1.1",
		"@nestjs/config": "^3.0.0",
		"@nestjs/core": "^10.1.1",
		"@nestjs/cqrs": "^10.0.3",
		"@nestjs/graphql": "^12.0.8",
		"@nestjs/platform-express": "^10.1.1",
		"@nestjs/swagger": "^7.1.2",
		"@nestjs/terminus": "^10.0.1",
		"@prisma/client": "^5.0.0",
		"apollo-server-express": "^3.12.0",
		"axios": "^1.4.0",
		"cache-manager": "^5.2.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"cli-progress": "^3.12.0",
		"exceljs": "^4.3.0",
		"geojson": "^0.5.0",
		"graphql": "^16.7.1",
		"graphql-geojson-scalar-types": "^0.1.0",
		"graphql-type-json": "^0.3.2",
		"keycloak-connect": "^21.1.2",
		"nest-keycloak-connect": "^1.9.2",
		"p-queue": "^6.6.2",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.8.1",
		"swagger-ui-express": "^4.6.3",
		"uuid": "^8.3.2",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.1.10",
		"@nestjs/schematics": "^10.0.1",
		"@nestjs/testing": "^10.1.1",
		"@swc/core": "^1.3.76",
		"@swc/jest": "^0.2.24",
		"@types/cli-progress": "^3.11.0",
		"@types/express": "^4.17.17",
		"@types/geojson": "^7946.0.10",
		"@types/jest": "27.4.1",
		"@types/node": "^18.0.0",
		"@types/proj4": "^2.5.2",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^5.35.1",
		"@typescript-eslint/parser": "^5.31.0",
		"dotenv": "^16.0.1",
		"eslint": "^8.45.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-cypress": "^2.13.3",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsdoc": "^39.9.1",
		"eslint-plugin-no-null": "^1.0.2",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.1",
		"jest": "^27.5.1",
		"jest-mock-extended": "^2.0.7",
		"lint-staged": "^13.2.3",
		"nestjs-console": "^9.0.0",
		"prettier": "^2.3.2",
		"prisma": "^5.0.0",
		"proj4": "^2.8.0",
		"run-script-webpack-plugin": "^0.1.1",
		"source-map-support": "^0.5.20",
		"supertest": "^6.2.4",
		"ts-jest": "^27.0.3",
		"ts-loader": "^9.3.1",
		"ts-node": "^10.8.1",
		"tsconfig-paths": "^4.0.0",
		"typescript": "^4.9.5",
		"webpack": "^5.74.0",
		"webpack-node-externals": "^3.0.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)sx?$": "@swc/jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	},
	"lint-staged": {
		"*.ts": [
			"prettier --verbose --write",
			"eslint --fix"
		],
		"*.{css,scss,json,md,yml}": [
			"prettier --verbose --write"
		]
	}
}
