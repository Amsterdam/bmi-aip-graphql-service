{
	"name": "bmi-aip-graphql-service",
	"version": "0.0.1",
	"description": "A GraphQL service developed with NestJS in service of the frontend of the \"Amsterdam Inspectie Portaal\" app",
	"author": "Gemeente Amsterdam",
	"license": "MPL-2.0",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:test": "nest start --watch",
		"start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"ci:test": "jest --ci --reporters=default",
		"ci:lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
		"prepare": "husky install",
		"console": "node dist/src/console.js",
		"prisma": "prisma",
		"generate:gisib-api-types": "node_modules/.bin/ts-node scripts/generate-gisib-api-types/index.ts"
	},
	"dependencies": {
		"@casl/ability": "^5.4.3",
		"@casl/prisma": "^1.1.0",
		"@golevelup/nestjs-graphql-request": "^0.1.13",
		"@nestjs/apollo": "^10.0.19",
		"@nestjs/axios": "^0.0.8",
		"@nestjs/common": "^8.4.7",
		"@nestjs/config": "^2.1.0",
		"@nestjs/core": "^8.4.7",
		"@nestjs/cqrs": "^8.0.5",
		"@nestjs/graphql": "^10.1.2",
		"@nestjs/platform-express": "^8.4.7",
		"@nestjs/terminus": "^8.1.1",
		"@prisma/client": "^3.15.2",
		"apollo-server-express": "^3.9.0",
		"cache-manager": "^4.1.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"cli-progress": "^3.11.2",
		"geojson": "^0.5.0",
		"graphql": "^16.6.0",
		"graphql-geojson-scalar-types": "^0.1.0",
		"graphql-type-json": "^0.3.2",
		"keycloak-connect": "^18.0.2",
		"nest-keycloak-connect": "^1.9.0",
		"p-queue": "^6.6.2",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.5.6",
		"uuid": "^8.3.2",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@nestjs/cli": "^8.2.8",
		"@nestjs/schematics": "^9.0.1",
		"@nestjs/testing": "^8.4.7",
		"@swc/core": "^1.3.24",
		"@swc/jest": "^0.2.24",
		"@types/cli-progress": "^3.11.0",
		"@types/express": "^4.17.13",
		"@types/geojson": "^7946.0.10",
		"@types/jest": "27.4.1",
		"@types/node": "^18.0.0",
		"@types/proj4": "^2.5.2",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^5.35.1",
		"@typescript-eslint/parser": "^5.48.1",
		"dotenv": "^16.0.1",
		"eslint": "^8.23.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.3.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsdoc": "^39.3.3",
		"eslint-plugin-no-null": "^1.0.2",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.1",
		"jest": "^27.5.1",
		"jest-mock-extended": "^2.0.7",
		"lint-staged": "^13.0.3",
		"nestjs-console": "^7.0.1",
		"prettier": "^2.3.2",
		"prisma": "^3.15.2",
		"proj4": "^2.8.0",
		"run-script-webpack-plugin": "^0.1.1",
		"source-map-support": "^0.5.20",
		"supertest": "^6.2.4",
		"ts-jest": "^27.0.3",
		"ts-loader": "^9.3.1",
		"ts-node": "^10.8.1",
		"tsconfig-paths": "^4.0.0",
		"typescript": "^4.7.4",
		"webpack": "^5.74.0",
		"webpack-node-externals": "^3.0.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)sx?$": "@swc/jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	},
	"lint-staged": {
		"*.ts": [
			"prettier --verbose --write",
			"eslint --fix"
		],
		"*.{css,scss,json,md,yml}": [
			"prettier --verbose --write"
		]
	}
}
